"strict": true,
"esModuleInterop": true,
"moduleResolution": "node",
"module": "esnext",
"jsx": "react-jsx",
"target": "es6",
"lib": ["dom", "dom.iterable", "esnext"],
"allowJs": true,
"skipLibCheck": true,
